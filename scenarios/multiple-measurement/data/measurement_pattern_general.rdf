            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Measured Value Expression</rdfs:label>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/core/Core/MeasurementInformationContentEntity -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/core/Core/MeasurementInformationContentEntity">
