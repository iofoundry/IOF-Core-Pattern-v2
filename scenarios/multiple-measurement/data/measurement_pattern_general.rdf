    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/core/Core/MeasuredValueExpression -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/core/Core/MeasuredValueExpression">
        <rdfs:subClassOf>
            <owl:Restriction>
