                <owl:someValuesFrom rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/MeasuredValueExpression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/isAbout"/>
