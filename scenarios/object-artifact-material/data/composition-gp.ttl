@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix bfo:  <http://purl.obolibrary.org/obo/> .
@prefix core: <http://example.org/core#> .

### Object properties
core:hasComponentPartAtSomeTime a owl:ObjectProperty ;
    rdfs:domain bfo:Object ;
    rdfs:range bfo:MaterialEntity .

core:isMadeOf a owl:ObjectProperty ;
    rdfs:domain bfo:MaterialEntity ;
    rdfs:range bfo:MaterialEntity .

### Axioms at the class level
bfo:Object rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty core:hasComponentPartAtSomeTime ;
    owl:someValuesFrom bfo:MaterialEntity
] .

bfo:MaterialEntity rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty core:isMadeOf ;
    owl:someValuesFrom bfo:MaterialEntity
] .

bfo:Object rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty core:isMadeOf ;
    owl:someValuesFrom bfo:MaterialEntity
] .
